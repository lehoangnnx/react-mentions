    "jest-enzyme": "^7.0.2",
    "jest-watch-typeahead": "^2.2.2",
    "preconstruct": "^1.0.0",
    "prettier": "^3.3.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "recompose": "^0.30.0",
    "regenerator-runtime": "^0.14.1",
    "style-loader": "^4.0.0",
    "substyle-jss": "^4.0.1",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
