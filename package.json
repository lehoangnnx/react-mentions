    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.3",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "core-js": "^3.37.1",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.8",
    "eslint": "^9.5.0",
